

def .flat-even from [
	.a() -> a;
	~null() -> a;
	.b() -> b;
	
	'~null() -> lst;
	~pair(b, lst) -> tail;
	~pair(a, tail) -> lst;
	
	~null() -> biglst;
	~pair(lst, biglst) -> biglst;
	
	final biglst;'
	final a;
] to [
	.a() -> a;
	.b() -> b;
	
	~null() -> lst;
	~pair(b, lst) -> tail;
	~pair(a, tail) -> lst;
	
	final lst;
]
